
@model tcc_demo.Models.Supplier

@using (Html.BeginForm("CadSupplier", "Supplier", FormMethod.Post, new { enctype = "multipart/form-data" }))
{

    <br />
    <div class="editor-label">
        @Html.LabelFor(m => m.nameSupplier)
    </div>

    <div class="editor-field">
        @Html.EditorFor(m => m.nameSupplier, new { htmlAttributes = new { @type = "text", } })
        @Html.ValidationMessageFor(model => model.nameSupplier, "", new { @class = "text-danger" })
    </div>
    <br />
    <div class="editor-label">
        @Html.LabelFor(m => m.cnpjSupplier)
    </div>
    <div class="editor-field">
        @Html.EditorFor(m => m.cnpjSupplier, new { htmlAttributes = new { @type = "text", } })
        @Html.ValidationMessageFor(model => model.cnpjSupplier, "", new { @class = "text-danger" })
       
    </div>
    <br />

    <input type="submit" value="Cadastrar" />
}
<br />
@ViewBag.msg

